{
  "name": "reporta-ya---sevilla",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently -n MONGO,SERVER -c green,cyan \"mongod\" \"node server/api.cjs\"",
    "dev": "concurrently -n MONGO,API,VITE -c green,cyan,magenta \"mongod\" \"cross-env PORT=3001 node server/api.cjs\" \"vite --port 3000 --host\"",
    "dev:api": "node server/api.cjs",
    "dev:vite": "vite",
    "build": "vite build",
    "preview": "vite preview --port 3000 --strictPort",
    "serve": "npm run build && node ./server/api.cjs",
    "create-admin": "node server/create_admin.cjs",
    "seed:users": "node server/seed_users.cjs",
    "server": "node server/api.cjs"
  },
  "dependencies": {
    "@google/genai": "^1.35.0",
    "@stripe/stripe-js": "^8.6.1",
    "@types/lodash": "^4.17.23",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.447.0",
    "mongodb": "^6.12.0",
    "nodemailer": "^8.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0",
    "stripe": "^20.2.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.16",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^10.1.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
